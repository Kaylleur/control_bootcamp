<script type="text/javascript">
	//<![CDATA[
	(function($) {
		
		$(document).ready(function() {
			
			$('.style8').each(function(){
				var dlParent = $(this);
				var dtParent = $('.replier_deplier',dlParent).parents('.style8_titre');
				dtParent.nextUntil('.style8_titre').each(function(){
					$(this).hide();
				});
				
				$('.replier_deplier',dlParent).click(function(){
					var btDeplierPlier = $(this);
					dtParent = btDeplierPlier.parents('.style8_titre');
					
					if (btDeplierPlier.hasClass('deplier')){
						btDeplierPlier.removeClass('deplier');
						btDeplierPlier.text("replier");
						dtParent.nextUntil('.style8_titre').each(function(){
							$(this).show();
						});
					} else {
						btDeplierPlier.addClass('deplier');
						btDeplierPlier.text("déplier");
						dtParent.nextUntil('.style8_titre').each(function(){
							$(this).hide();
						});
					}
					return false;
				});
			});
			
		});
		
	})(jQuery);
	//]]>
</script>


